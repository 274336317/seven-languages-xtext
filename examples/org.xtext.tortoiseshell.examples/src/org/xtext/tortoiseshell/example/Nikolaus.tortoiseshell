begin
  val length = 150
  val diagonal = length * sqrt(2)
  square(length)
  turnRight(45)
  forward(diagonal)
  turnLeft(90)
  forward(diagonal / 2)
  turnLeft(90)
  forward(diagonal / 2)
  turnLeft(90)
  forward(diagonal)
end

sub square
  double length
begin
  for(i: 1..4) {
    forward(length)
    turnRight(90)
  }
end