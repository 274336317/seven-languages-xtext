begin
  val length = 150
  val diagonal = length * sqrt(2)
  forward(length)
  turnRight(90)
  forward(length)
  turnRight(90)
  forward(length)
  turnRight(90)
  forward(length)
  turnRight(135)
  forward(diagonal)
  turnLeft(90)
  forward(diagonal / 2)
  turnLeft(90)
  forward(diagonal / 2)
  turnLeft(90)
  forward(diagonal)
end
