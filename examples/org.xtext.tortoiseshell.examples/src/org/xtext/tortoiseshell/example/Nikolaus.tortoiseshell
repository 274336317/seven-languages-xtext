begin
  val length = 150
  val diagonal = length * sqrt(2)
  lineWidth = 5 
  lineColor = black
  forward(length)
  turnRight(90)
  forward(length)
  turnRight(90)
  forward(length)
  turnRight(90)
  forward(length)
  turnRight(135)
  forward(diagonal)
  turnLeft(90)
  forward(diagonal / 2)
  turnLeft(90)
  forward(diagonal / 2)
  turnLeft(90)
  forward(diagonal)
end

sub diagonal
	double length 
begin
	length * sqrt(2)
end